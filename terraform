pipeline {
    agent {label "OPENJDK-11-2"}
    stages {
           stage('clone'){
            steps {
                git url:'https://github.com/GitPracticeRepo/dotnetcore-docs-hello-world.git', branch :'master'
                 }
                }
        stage('build'){
            steps {
                sh 'dotnet build dotnetcoresample.csproj'
                 }
             }
             
    }
}